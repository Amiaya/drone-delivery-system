# -------------------------------
# üöÄ Server Configuration
# -------------------------------
PORT=3005                    # Application port (default: 3005)

# -------------------------------
# üóÑÔ∏è PostgreSQL Database Configuration
# -------------------------------
POSTGRES_DB=postgres          # Database name
TEST_POSTGRES_DB=test         # Test database name (for automated tests)
POSTGRES_USER=postgres        # Database user
POSTGRES_PASSWORD=your_password_here  # Database password
POSTGRES_HOST=localhost       # Database host (use your production DB host in prod)
POSTGRES_PORT=5432            # Database port (default: 5432)
POSTGRES_SCHEMA=drones        # Schema name

